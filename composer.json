{
    "name": "bow/preishoheit",
    "description": "Dynamic price adjustments for Shopware products based on Preishoheit API",
    "type": "shopware-platform-plugin",
    "license": "proprietary",
    "version": "1.0.0",
    "require": {
        "shopware/core": "~6.6.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.4",
        "symfony/http-client": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "BOW\\Preishoheit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BOW\\Preishoheit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "shopware-plugin-class": "BOW\\Preishoheit\\BOWPreishoheit",
        "label": {
            "de-DE": "Preishoheit",
            "en-GB": "Price Authority"
        }
    }
}
